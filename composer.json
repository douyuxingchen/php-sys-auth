{
    "name": "douyuxingchen/php-sys-auth",
    "description": "PHP外部系统授权签名验证库。",
    "type": "library",
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "Douyuxingchen\\PhpSysAuth\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "libin",
            "email": "libin@douyuxingchen.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "laravel/framework": "^6.0",
         "predis/predis": "^1.1"
    }
}
